{
  "name": "vue-sort",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-sort.umd.js",
  "module": "./dist/vue-sort.es.js",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/vue-sort.es.js",
      "require": "./dist/vue-sort.umd.js"
    },
    "./dist/vue-sort.css": {
      "import": "./dist/vue-sort.css",
      "require": "./dist/vue-sort.css"
    }
  },
  "types": "./dist/main.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:watch": "turbowatch ./turbowatch.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "path": "^0.12.7",
    "rollup-plugin-typescript2": "^0.34.1",
    "turbowatch": "^2.29.4",
    "typescript": "^5.1.6",
    "vite": "^4.0.0",
    "vite-plugin-css-injected-by-js": "^3.2.1",
    "vite-plugin-dts": "^1.7.1",
    "vue-tsc": "^1.0.11"
  }
}
